
# RevenueCat Configuration
# Get these from: https://app.revenuecat.com/settings/api-keys
# Each platform has its own API key
VITE_REVENUECAT_ANDROID_API_KEY="your_revenuecat_android_api_key"
VITE_REVENUECAT_IOS_API_KEY="your_revenuecat_ios_api_key"
VITE_REVENUECAT_WEB_API_KEY="your_revenuecat_web_api_key"
# IMPORTANT: Get this from: https://app.revenuecat.com/webhooks
# After creating the webhook, copy the authorization header value here
# Webhook URL: https://your-project.supabase.co/functions/v1/revenuecat-webhook
REVENUECAT_WEBHOOK_SECRET="your_webhook_secret_from_revenuecat_dashboard"

RESEND_API_KEY="your_resend_api_key"

TWILIO_ACCOUNT_SID="your_twilio_account_sid"
TWILIO_AUTH_TOKEN="your_twilio_auth_token"
TWILIO_PHONE_NUMBER="+1234567890"

EXPO_PUBLIC_SUPABASE_URL="https://your-project.supabase.co"
EXPO_PUBLIC_SUPABASE_KEY="your_supabase_anon_key"

# Supabase MCP Server Authentication
# Get this from: https://app.supabase.com -> Settings -> API -> Service Role Key
SUPABASE_SERVICE_ROLE_KEY="your_supabase_service_role_key"
VITE_SUPABASE_PROJECT_ID="your_supabase_project_id"
VITE_SUPABASE_PUBLISHABLE_KEY="your_supabase_publishable_key"
VITE_SUPABASE_URL="https://your-project.supabase.co"

# Stripe Configuration
# Get these from: https://dashboard.stripe.com/apikeys
STRIPE_SECRET_KEY="your_stripe_secret_key"

# Stripe Webhook Secret (from https://dashboard.stripe.com/webhooks)
# Webhook URL: https://your-project.supabase.co/functions/v1/stripe-webhook
# Listen to events: checkout.session.completed, payment_intent.succeeded
STRIPE_WEBHOOK_SECRET="your_stripe_webhook_secret"

# App URL for redirects (production URL)
APP_URL="https://app.tradiemate.com.au"
